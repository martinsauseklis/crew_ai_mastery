    {"id": 25, "category": "Neural Network Architecture", "difficulty": "beginner", "question": "What is an Epoch?", "answer": "One complete pass through the entire training dataset during the training process.", "tags": ["epoch", "training", "iteration"]},
    {"id": 26, "category": "Neural Network Architecture", "difficulty": "beginner", "question": "What is a Batch?", "answer": "A subset of the training data processed together before updating model weights.", "tags": ["batch", "training", "data"]},
    {"id": 27, "category": "Neural Network Architecture", "difficulty": "beginner", "question": "What is Batch Size?", "answer": "The number of training examples used in one iteration of model training.", "tags": ["batch size", "hyperparameter", "training"]},
    {"id": 28, "category": "Neural Network Architecture", "difficulty": "intermediate", "question": "What is Learning Rate?", "answer": "A hyperparameter that controls how much the model weights are adjusted during training in response to the calculated error.", "tags": ["learning rate", "hyperparameter", "optimization"]},
    {"id": 29, "category": "Neural Network Architecture", "difficulty": "intermediate", "question": "What is a Loss Function?", "answer": "A mathematical function that measures how well the model's predictions match the actual values, guiding the optimization process.", "tags": ["loss function", "optimization", "error"]},
    {"id": 30, "category": "Neural Network Architecture", "difficulty": "intermediate", "question": "What is Gradient Descent?", "answer": "An optimization algorithm that iteratively adjusts model parameters to minimize the loss function by moving in the direction of steepest descent.", "tags": ["gradient descent", "optimization", "algorithm"]},
    {"id": 31, "category": "Activation Functions", "difficulty": "intermediate", "question": "What is the purpose of Activation Functions?", "answer": "Mathematical functions applied to neuron outputs to introduce non-linearity, enabling neural networks to learn complex patterns.", "tags": ["activation", "non-linearity", "functions"]},
    {"id": 32, "category": "Activation Functions", "difficulty": "intermediate", "question": "What is ReLU (Rectified Linear Unit)?", "answer": "Activation function defined as f(x) = max(0, x), which outputs the input if positive, otherwise zero. Most popular due to computational efficiency.", "tags": ["ReLU", "activation", "popular"]},
    {"id": 33, "category": "Activation Functions", "difficulty": "intermediate", "question": "What is the Sigmoid Function?", "answer": "Activation function that squashes input values to range (0, 1), defined as f(x) = 1/(1 + e^(-x)). Often used in binary classification output layers.", "tags": ["sigmoid", "activation", "classification"]},
    {"id": 34, "category": "Activation Functions", "difficulty": "intermediate", "question": "What is the Tanh Function?", "answer": "Hyperbolic tangent activation function that maps inputs to range (-1, 1), providing zero-centered outputs unlike sigmoid.", "tags": ["tanh", "activation", "zero-centered"]},
    {"id": 35, "category": "Activation Functions", "difficulty": "intermediate", "question": "What is the Softmax Function?", "answer": "Activation function used in output layers for multi-class classification, converting raw scores into probability distribution summing to 1.", "tags": ["softmax", "activation", "multi-class"]},
    {"id": 36, "category": "Activation Functions", "difficulty": "advanced", "question": "What is Leaky ReLU?", "answer": "Variant of ReLU that allows small negative values (f(x) = x if x > 0, else alpha*x where alpha is small), preventing dying neuron problem.", "tags": ["leaky ReLU", "activation", "variant"]},
    {"id": 37, "category": "Activation Functions", "difficulty": "advanced", "question": "What is the Vanishing Gradient Problem?", "answer": "Issue where gradients become extremely small during backpropagation, making it difficult to train deep networks, especially with sigmoid/tanh.", "tags": ["vanishing gradient", "problem", "training"]},
    {"id": 38, "category": "Activation Functions", "difficulty": "advanced", "question": "What is the Exploding Gradient Problem?", "answer": "Opposite of vanishing gradients, where gradients become extremely large, causing unstable training and numerical overflow.", "tags": ["exploding gradient", "problem", "training"]},
    {"id": 39, "category": "Activation Functions", "difficulty": "advanced", "question": "What is the Dying ReLU Problem?", "answer": "Issue where neurons using ReLU activation can get stuck outputting zero for all inputs, stopping learning completely.", "tags": ["dying ReLU", "problem", "neurons"]},
    {"id": 40, "category": "Activation Functions", "difficulty": "advanced", "question": "What is ELU (Exponential Linear Unit)?", "answer": "Activation function that smoothly handles negative values, reducing bias shift and bringing mean activations closer to zero.", "tags": ["ELU", "activation", "advanced"]},
    {"id": 41, "category": "Convolutional Neural Networks", "difficulty": "intermediate", "question": "What is a Convolutional Neural Network (CNN)?", "answer": "A type of neural network specialized for processing grid-like data such as images, using convolutional layers to detect spatial patterns.", "tags": ["CNN", "computer vision", "architecture"]},
    {"id": 42, "category": "Convolutional Neural Networks", "difficulty": "intermediate", "question": "What is the Convolution Operation?", "answer": "Mathematical operation that applies a filter/kernel to input data by sliding it across and computing dot products to extract features.", "tags": ["convolution", "operation", "CNN"]},
    {"id": 43, "category": "Convolutional Neural Networks", "difficulty": "intermediate", "question": "What is a Filter/Kernel?", "answer": "Small matrix of weights that slides across input data in CNNs to detect specific features like edges, textures, or patterns.", "tags": ["filter", "kernel", "CNN"]},
    {"id": 44, "category": "Convolutional Neural Networks", "difficulty": "intermediate", "question": "What is a Feature Map?", "answer": "The output produced by applying a convolutional filter to input data, highlighting where specific features are detected.", "tags": ["feature map", "CNN", "output"]},
    {"id": 45, "category": "Convolutional Neural Networks", "difficulty": "intermediate", "question": "What is a Pooling Layer?", "answer": "Layer in CNNs that reduces spatial dimensions of feature maps, decreasing computational load while retaining important information.", "tags": ["pooling", "CNN", "downsampling"]},
    {"id": 46, "category": "Convolutional Neural Networks", "difficulty": "intermediate", "question": "What is Max Pooling?", "answer": "Pooling operation that selects the maximum value from each region, preserving the strongest activations.", "tags": ["max pooling", "CNN", "operation"]},
    {"id": 47, "category": "Convolutional Neural Networks", "difficulty": "intermediate", "question": "What is Average Pooling?", "answer": "Pooling operation that computes the average value in each region, providing a smoother down-sampling.", "tags": ["average pooling", "CNN", "operation"]},
    {"id": 48, "category": "Convolutional Neural Networks", "difficulty": "intermediate", "question": "What is Stride?", "answer": "The number of pixels by which the filter moves across the input during convolution or pooling operations.", "tags": ["stride", "CNN", "parameter"]},
    {"id": 49, "category": "Convolutional Neural Networks", "difficulty": "intermediate", "question": "What is Padding?", "answer": "Adding extra pixels (usually zeros) around the input borders to control output size and preserve edge information.", "tags": ["padding", "CNN", "preprocessing"]},
